<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.cloudstorage.dao.TPADAO">

    <select id="selectToken" parameterType="java.lang.String" resultType="java.lang.Integer">
        select COUNT(*) from tpa where token = #{token}
    </select>

</mapper>
